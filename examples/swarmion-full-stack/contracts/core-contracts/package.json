{
  "name": "@swarmion-full-stack/core-contracts",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "contracts": true,
  "scripts": {
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts .",
    "package": "rm -rf dist && pnpm package-cjs && pnpm package-esm && pnpm package-types",
    "package-cjs": "NODE_ENV=cjs pnpm transpile --out-dir dist/cjs --source-maps",
    "package-esm": "NODE_ENV=esm pnpm transpile --out-dir dist/esm --source-maps",
    "package-types": "ttsc -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js src",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "transpile": "babel src --extensions .ts --quiet",
    "watch": "rm -rf dist && concurrently 'pnpm:package-* --watch'"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@swarmion/serverless-contracts": "^0.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/node": "^17.0.45",
    "@vitest/coverage-c8": "^0.23.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.2.2",
    "dependency-cruiser": "^11.10.0",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "vitest": "^0.23.1"
  }
}
