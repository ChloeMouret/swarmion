{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "bootstrap",
          "build",
          "package",
          "test-circular",
          "test-linter",
          "test-type",
          "test-unit"
        ],
        "cacheDirectory": "nx-cache"
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": ["!{projectRoot}/**/*.test.tsx?"]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^build"]
    },
    "deploy": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^deploy", "build", "bootstrap"]
    },
    "deploy-staging": {
      "inputs": ["production", "^production"],
      "dependsOn": ["build", "^deploy-staging", "bootstrap-staging"]
    },
    "deploy-production": {
      "inputs": ["production", "^production"],
      "dependsOn": ["build", "^deploy-production", "bootstrap-production"]
    },
    "test-linter": {
      "inputs": ["default", "^production", "{workspaceRoot}/.eslintrc.cjs"]
    },
    "test-unit": {
      "inputs": ["default", "^production"],
      "outputs": ["{projectRoot}/coverage"]
    },
    "test-type": {
      "inputs": ["default", "^production"]
    },
    "test-circular": {
      "inputs": ["default", "^production"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
