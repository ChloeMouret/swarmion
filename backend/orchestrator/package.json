{
  "name": "@swarmion/backend-orchestrator",
  "private": true,
  "version": "0.12.0",
  "license": "MIT",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy-production": "serverless deploy --stage production",
    "deploy-staging": "serverless deploy --stage staging",
    "deployedCommit": "serverless deployedCommit",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint:fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "remove": "serverless remove",
    "remove-production": "serverless remove --stage production",
    "remove-staging": "serverless remove --stage staging",
    "sls-info": "serverless info --verbose",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@swarmion/orchestrator-contracts": "^0.12.0",
    "@swarmion/serverless-cdk-plugin": "^0.12.0",
    "@swarmion/serverless-configuration": "^0.12.0",
    "@swarmion/serverless-contracts": "^0.12.0",
    "@swarmion/serverless-helpers": "^0.12.0",
    "@swarmion/serverless-plugin": "^0.12.0",
    "aws-cdk-lib": "^2.46.0",
    "aws-sdk": "^2.1232.0",
    "dynamodb-toolbox": "^0.4.3",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@swarmion/eslint-plugin": "^0.12.0",
    "@types/node": "^18.8.5",
    "@vitest/coverage-c8": "^0.23.4",
    "constructs": "^10.1.131",
    "dependency-cruiser": "^11.16.1",
    "esbuild": "^0.14.54",
    "esbuild-visualizer": "^0.3.1",
    "eslint": "^8.25.0",
    "serverless": "^3.23.0",
    "serverless-analyze-bundle-plugin": "^1.2.1",
    "serverless-custom-iam-roles-per-function": "^1.0.1",
    "serverless-esbuild": "^1.33.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "6.15.5",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.23.4"
  }
}
