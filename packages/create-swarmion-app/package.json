{
  "name": "create-swarmion-app",
  "description": "Create an app based on the Swarmion template.",
  "version": "0.5.6",
  "contributors": [
    "MaximeVivier",
    "guillaumeduboc"
  ],
  "license": "MIT",
  "homepage": "https://www.swarmion.dev",
  "bugs": "https://github.com/swarmion/swarmion/issues",
  "repository": "swarmion/swarmion.git",
  "keywords": [
    "Serverless",
    "Swarmion",
    "Typescript",
    "app",
    "create"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "dev": "ncc build ./src/index.ts -w -o dist/",
    "lint-fix": "yarn linter-base-config --fix",
    "lint-fix-all": "yarn lint-fix .",
    "linter-base-config": "eslint --ext=js,ts .",
    "test": "yarn test-linter && yarn test-type",
    "test-linter": "yarn linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "transpile": "babel src --extensions .ts --quiet --copy-files",
    "watch": "rm -rf dist && concurrently 'yarn:package-* --watch'"
  },
  "bin": "./dist/index.js",
  "devDependencies": {
    "@types/async-retry": "^1.4.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^17.0.35",
    "@types/prompts": "^2.0.1",
    "@types/tar": "^4.0.3",
    "@types/validate-npm-package-name": "^3.0.0",
    "@vercel/ncc": "^0.33.4",
    "async-retry": "^1.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "concurrently": "^7.1.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^8.14.0",
    "got": "^10.7.0",
    "prettier": "^2.6.2",
    "prompts": "^2.1.0",
    "tar": "^4.4.10",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^3.0.0"
  }
}
