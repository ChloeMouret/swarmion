{
  "name": "create-swarmion-app",
  "description": "Create an app based on the Swarmion template.",
  "version": "0.6.0-alpha.1",
  "contributors": [
    "MaximeVivier",
    "guillaumeduboc"
  ],
  "license": "MIT",
  "homepage": "https://www.swarmion.dev",
  "bugs": "https://github.com/swarmion/swarmion/issues",
  "repository": "swarmion/swarmion.git",
  "keywords": [
    "Serverless",
    "Swarmion",
    "Typescript",
    "app",
    "create"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "lint-fix": "yarn linter-base-config --fix",
    "lint-fix-all": "yarn lint-fix .",
    "linter-base-config": "eslint --ext=js,ts .",
    "package": "rm -rf dist && yarn package-cjs",
    "package-cjs": "NODE_ENV=cjs yarn transpile --out-dir dist --source-maps",
    "test": "yarn test-linter && yarn test-type",
    "test-linter": "yarn linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "transpile": "babel src --extensions .ts --quiet --copy-files",
    "watch": "rm -rf dist && concurrently 'yarn:package-* --watch'"
  },
  "bin": "./dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/async-retry": "^1.4.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^17.0.29",
    "@types/prompts": "^2.0.1",
    "@types/tar": "^4.0.3",
    "@types/validate-npm-package-name": "^3.0.0",
    "async-retry": "^1.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "concurrently": "^7.1.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^8.14.0",
    "got": "^10.7.0",
    "prettier": "^2.6.2",
    "prompts": "^2.1.0",
    "tar": "^4.4.10",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^3.0.0"
  }
}
